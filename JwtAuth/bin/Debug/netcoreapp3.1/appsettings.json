{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SwaggerOption": {
    "Enable": true, //ÊÇ·ñ¿ªÆôSwagger 
    "RoutePrefix": "swagger",
    "Description": "this is swagger for netcore",
    "Title": "Demo swagger",
    "Version": "first version",
    "TermsOfService": "netcore.com",
    //"IsDarkTheme": true,
    "Contact": {
      "Email": "swagger@netcore.com",
      "Name": "swagger",
      "Url": "swagger@netcore.com"
    },
    "License": {
      "Name": "",
      "Url": ""
    }
  },
  "FeatureManagement": {
    "FeatureA": true, // Feature flag set to on
    "FeatureB": false, // Feature flag set to off
    "FeatureC": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "FeatureD": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "03/11/2021 07:32:00 GMT",
            "End": "03/11/2021 07:35:00 GMT"
          }
        }
      ]
    },
    "BrowserFlag": {
      "EnabledFor": [
        {
          "Name": "BrowserFilter",
          "Parameters": {
            "AllowedBrowsers": [
              "Edg"
            ]
          }
        }
      ]
    }
  }
}
